<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.hibernate.pojo.YfuserEntity">
        <table name="yfuser" schema="blog" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="username">
                <column name="username" nullable="false"/>
            </basic>
            <basic name="password">
                <column name="password" nullable="false"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
